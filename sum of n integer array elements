#include<stdio.h>
#include<conio.h>
int sum(int ele[],int n)
{
int i,sum=0;
for(i=0;i<n;i++)
sum=sum+ele[i];
return sum;
}
void main()
{
int ele[50],n,i,ans;
printf("enter the value of n\n");
scanf("%d",&n);
for(i=0;i<n;i++)
scanf("%d",&ele[i]);
ans=sum(ele[],n);
printf("the sum of n integers is %d",ans);
getch();
}
